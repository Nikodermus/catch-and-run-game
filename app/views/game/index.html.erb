<div class="border">
    <div class="panel">
        <div class="left">
            <img src="/assets/images/lost_soul.gif" alt="">
            <span id="SoulsCount">00
            </span>
        </div>
        <div class="right">
            <figure class="lives" id="LifeCount">
            </figure>
            <button id="PauseGame" class="square">
                <div>II
                </div>
            </button>
        </div>
        <figure>
            <img src="/assets/images/3_lifes.gif" alt="" id="PlayerStatus">
            <img src="" alt="" id="PowerActive">
        </figure>
    </div>
</div>
<div class="border">
</div>
<img src="/assets/images/corner.png" alt="" class="corner">
<img src="/assets/images/corner.png" alt="" class="corner">
